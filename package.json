{
  "name": "homepage",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.21.0",
  "description": "TechLead Inc. corporate homepage",
  "scripts": {
    "dev": "pnpm -r --parallel --stream dev",
    "build": "pnpm --filter worker build && pnpm --filter web build",
    "build:web": "pnpm --filter web build",
    "build:worker": "pnpm --filter worker build",
    "preview": "pnpm --filter web preview",
    "check": "pnpm -r run check",
    "check:ci": "pnpm -r run check:ci",
    "deploy:worker": "pnpm --filter worker deploy"
  },
  "devEngines": {
    "runtime": [
      {
        "name": "node",
        "version": "^24.10.0",
        "onFail": "download"
      }
    ]
  },
  "devDependencies": {}
}
